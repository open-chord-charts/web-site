{
  "name": "openchordcharts-web-site",
  "version": "0.0.6",
  "description": "Open Chord Charts - web site",
  "scripts": {
    "analyze": "webpack --progress --colors --display-modules --display-reasons --sort-modules-by size",
    "build:prod": "webpack --config webpack-prod.config.js --production --progress --colors",
    "clean": "rm -rf dist stats.json",
    "ctags": "ctags --recurse=yes --exclude=node_modules .",
    "start": "webpack-dev-server --hot --progress --colors --inline",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openchordcharts/openchordcharts-web-ui.git"
  },
  "author": "Christophe Benz <christophe.benz@gmail.com>",
  "license": "AGPLv3+",
  "bugs": {
    "url": "https://github.com/openchordcharts/openchordcharts-web-site/issues"
  },
  "dependencies": {
    "babel-core": "^4.7.16",
    "classnames": "^1.2.0",
    "immutable": "^3.7.1",
    "isomorphic-fetch": "^2.0.0",
    "normalize.css": "^3.0.3",
    "querystringify": "0.0.2",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "react-style": "^0.5.1",
    "transducers.js": "^0.2.3"
  },
  "devDependencies": {
    "babel-loader": "^4.3.0",
    "css-loader": "^0.9.1",
    "eslint": "^0.18.0",
    "extract-text-webpack-plugin": "^0.5.0",
    "file-loader": "^0.8.1",
    "html-loader": "^0.2.3",
    "html-webpack-plugin": "^1.1.0",
    "react-hot-loader": "^1.2.4",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.8.0",
    "webpack-error-notification": "^0.1.4"
  },
  "private": true
}
